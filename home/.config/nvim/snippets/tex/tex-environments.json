	"environment-iff": {
		"prefix": "iff",
		"body": [
			"\\begin{itemize}",
			"\t\\item[$(\\Rightarrow)$]",
			"\t\t$0",
			"\t\\item[$(\\Leftarrow)$]",
			"\\end{itemize}"
		],
		"description": "Environment: Iff"
	},
	"environment-iff-cup": {
		"prefix": "iffc",
		"body": [
