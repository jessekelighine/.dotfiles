		],
		"description": "Environment: Align"
	},
	"environment-gather": {
		"prefix": "ga",
		"body": [
			"\\begin{gather*}",
			"\t$0",
			"\\end{gather*}"
		],
		"description": "Environment: Gather"
	},
	"environment-gather": {
		"prefix": "ga",
		"body": [
