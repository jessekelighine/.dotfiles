			"\t\\item[$(\\subset)$]",
			"\t\t$0",
			"\t\\item[$(\\supset)$]",
			"\\end{itemize}"
		],
		"description": "Environment: Iff Cup"
	},
	"environment-frame": {
		"prefix": "fr",
		"body": [
			"\\begin{frame}{$1}",
